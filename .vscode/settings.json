    "Settings",
    "InterfaceOptions_AddCategory",
    "GameFontHighlightLarge",
    "GameFontHighlightSmall",
    "SlashCmdList",
    "hash_SlashCmdList",
    "NORMAL_FONT_COLOR_CODE",
    "FONT_COLOR_CODE_CLOSE",
    "CLOSE",
    "GameFontDisableSmall",
    "GameFontNormalSmall",
    "SetDesaturation",
    "ColorPickerFrame",
    "OpacitySliderFrame",
    "ChatFontNormal",
    "OKAY",
    "NOT_BOUND",
    "IsSecureCmd",
    "NUM_CHAT_WINDOWS",
